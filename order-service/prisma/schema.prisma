generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order {
  id         Int      @id @default(autoincrement())
  user_id    Int
  status     String   @default("Pending")
  quantity   Int      @default(1)
  product_id Int
  createdAt  DateTime @default(now())
}
